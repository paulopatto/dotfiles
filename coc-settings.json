{
  "suggest.noselect": false,
  "codeLens.enable": true,
  "codeLens.position": "eol",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql"
  ],
  "extensions": [
    "coc-json",
    "coc-tsserver"
  ],
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "eslint.packageManager": "npm"
}
